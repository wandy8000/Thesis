@article{Xu2003c,
abstract = {The organic-inorganic hybrid [(CH(3))(3)NCH(2)CH(2)NH(3)]SnI(4) presents a layered perovskite structure, templated by an organic dication containing both a primary and a quaternary ammonium group. Due to the high charge density and small size of the organic cation, the separation of the perovskite layers is small and short iodine-iodine contacts of 4.19 A are formed between the layers. Optical thin-film measurements on this compound indicate a significant red shift of the exciton peak (630 nm) associated with the band gap, as compared with other SnI(4)(2)(-)-based layered perovskite structures.},
